// << 05.08.2022, Маскаев П.Ю., КРОК, Jira№ A2105505-324
&НаСервере
Процедура КР_ПриСозданииНаСервереДополнительно(Отказ, СтандартнаяОбработка)
	
	// Код
	КодЭлемент = КР_МетодыМодификацииФорм.ВставитьЭлементФормы(
		ЭтотОбъект, "Список.Код", Элементы.Список, Элементы.Наименование);
	
	// КР_Сезон
	КР_СезонЭлемент = КР_МетодыМодификацииФорм.ВставитьЭлементФормы(
		ЭтотОбъект, "Список.КР_Сезон", Элементы.Список);
	
	// КР_ВремяГодаКалендарное
	КР_ВремяГодаКалендарноеЭлемент = КР_МетодыМодификацииФорм.ВставитьЭлементФормы(
		ЭтотОбъект, "Список.КР_ВремяГодаКалендарное", Элементы.Список);
		
	// << 12.01.2023, Федоров Д.Е., КРОК, Jira№ A2105505-775
	// КР_Приоритет
	КР_ПриоритетЭлемент = КР_МетодыМодификацииФорм.ВставитьЭлементФормы(
		ЭтотОбъект, "Список.КР_Приоритет", Элементы.Список);
	// >> 12.01.2023, Федоров Д.Е., КРОК, Jira№ A2105505-775
	
КонецПроцедуры // >> 05.08.2022, Маскаев П.Ю., КРОК, Jira№ A2105505-324
        
#КонецОбласти

#КонецОбласти

