////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ДОБАВИТЬКДАТЕ(НАЧАЛОПЕРИОДА(&amp;НачалоПериода, ДЕНЬ), ДЕНЬ, П1.Ключ + П2.Ключ * 10 + П3.Ключ * 100) КАК Период
ПОМЕСТИТЬ врт_ДниОтчета
ИЗ
	врт_Порядки КАК П1
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ врт_Порядки КАК П2
		ПО (ИСТИНА)
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ врт_Порядки КАК П3
		ПО (ИСТИНА)
ГДЕ
	П1.Ключ + П2.Ключ * 10 + П3.Ключ * 100 &lt;= РАЗНОСТЬДАТ(&amp;НачалоПериода, &amp;КонецПериода, ДЕНЬ)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ДОБАВИТЬКДАТЕ(НАЧАЛОПЕРИОДА(&amp;НачалоПериода, ДЕНЬ), ЧАС, П1.Ключ + П2.Ключ * 10) КАК Период
ПОМЕСТИТЬ врт_ЧасыОтчета
ИЗ
	врт_Порядки КАК П1
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ врт_Порядки КАК П2
		ПО (ИСТИНА)
ГДЕ
	П1.Ключ + П2.Ключ * 10 &lt;= РАЗНОСТЬДАТ(&amp;НачалоПериода, &amp;КонецПериода, ЧАС)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	АрхивСообщений.ТипСообщения КАК ТипСообщения,
	АрхивСообщений.ТипДанных КАК ТипДанных,
	КОЛИЧЕСТВО(РАЗЛИЧНЫЕ АрхивСообщений.КлючСообщения) КАК КоличествоПакетов,
	НАЧАЛОПЕРИОДА(АрхивСообщений.ДатаАрхивирования, ЧАС) КАК ПериодЧас,
	НАЧАЛОПЕРИОДА(АрхивСообщений.ДатаАрхивирования, ДЕНЬ) КАК ПериодДень,
	СРЕДНЕЕ(ВЫБОР
			КОГДА АрхивСообщений.ДатаРегистрации = ДАТАВРЕМЯ(1, 1, 1)
					ИЛИ АрхивСообщений.ДатаАрхивирования = ДАТАВРЕМЯ(1, 1, 1)
				ТОГДА 0
			ИНАЧЕ РАЗНОСТЬДАТ(АрхивСообщений.ДатаРегистрации, АрхивСообщений.ДатаАрхивирования, СЕКУНДА)
		КОНЕЦ) КАК Дельта1,
	СРЕДНЕЕ(ВЫБОР
			КОГДА АрхивСообщений.ДатаОтправки = ДАТАВРЕМЯ(1, 1, 1)
					ИЛИ АрхивСообщений.ДатаАрхивирования = ДАТАВРЕМЯ(1, 1, 1)
				ТОГДА 0
			ИНАЧЕ РАЗНОСТЬДАТ(АрхивСообщений.ДатаОтправки, АрхивСообщений.ДатаАрхивирования, СЕКУНДА)
		КОНЕЦ) КАК Дельта2,
	СРЕДНЕЕ(ВЫБОР
			КОГДА АрхивСообщений.ТипСообщения = ЗНАЧЕНИЕ(Перечисление.КР_ТипыСообщенийОбмена.ВходящееСообщение)
				ТОГДА ВЫБОР
						КОГДА АрхивСообщений.ДатаПолучения = ДАТАВРЕМЯ(1, 1, 1)
								ИЛИ АрхивСообщений.ДатаАрхивирования = ДАТАВРЕМЯ(1, 1, 1)
							ТОГДА 0
						ИНАЧЕ РАЗНОСТЬДАТ(АрхивСообщений.ДатаПолучения, АрхивСообщений.ДатаАрхивирования, СЕКУНДА)
					КОНЕЦ
			ИНАЧЕ 0
		КОНЕЦ) КАК Дельта3,
	СРЕДНЕЕ(ВЫБОР
			КОГДА АрхивСообщений.ТипСообщения = ЗНАЧЕНИЕ(Перечисление.КР_ТипыСообщенийОбмена.ВходящееСообщение)
				ТОГДА ВЫБОР
						КОГДА АрхивСообщений.ДатаОбработки = ДАТАВРЕМЯ(1, 1, 1)
								ИЛИ АрхивСообщений.ДатаАрхивирования = ДАТАВРЕМЯ(1, 1, 1)
							ТОГДА 0
						ИНАЧЕ РАЗНОСТЬДАТ(АрхивСообщений.ДатаОбработки, АрхивСообщений.ДатаАрхивирования, СЕКУНДА)
					КОНЕЦ
			ИНАЧЕ 0
		КОНЕЦ) КАК Дельта4
ПОМЕСТИТЬ врт_ДанныеРегистра2
ИЗ
	РегистрСведений.КР_АрхивСообщений КАК АрхивСообщений
ГДЕ
	АрхивСообщений.ДатаАрхивирования МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
{ГДЕ
	АрхивСообщений.ТипСообщения.*}

СГРУППИРОВАТЬ ПО
	АрхивСообщений.ТипДанных,
	НАЧАЛОПЕРИОДА(АрхивСообщений.ДатаАрхивирования, ЧАС),
	НАЧАЛОПЕРИОДА(АрхивСообщений.ДатаАрхивирования, ДЕНЬ),
	АрхивСообщений.ТипСообщения
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	врт_ЧасыОтчета.Период КАК Период,
	ЕСТЬNULL(врт_ДанныеРегистра.ТипДанных, "") КАК ТипДанных,
	ЕСТЬNULL(врт_ДанныеРегистра.КоличествоПакетов, 0) КАК КоличествоПакетов,
	ЕСТЬNULL(врт_ДанныеРегистра.Дельта1, 0) КАК Дельта1,
	ЕСТЬNULL(врт_ДанныеРегистра.Дельта2, 0) КАК Дельта2,
	ЕСТЬNULL(врт_ДанныеРегистра.Дельта3, 0) КАК Дельта3,
	ЕСТЬNULL(врт_ДанныеРегистра.Дельта4, 0) КАК Дельта4
{ВЫБРАТЬ
	Период,
	ТипДанных,
	КоличествоПакетов,
	Дельта1,
	Дельта2,
	Дельта3,
	Дельта4}
ИЗ
	врт_ЧасыОтчета КАК врт_ЧасыОтчета
		ЛЕВОЕ СОЕДИНЕНИЕ врт_ДанныеРегистра2 КАК врт_ДанныеРегистра
		ПО врт_ЧасыОтчета.Период = врт_ДанныеРегистра.ПериодЧас
ГДЕ
	&amp;РежимОтчета = 0
{ГДЕ
	врт_ЧасыОтчета.Период,
	(ЕСТЬNULL(врт_ДанныеРегистра.ТипДанных, "")) КАК ТипДанных,
	(ЕСТЬNULL(врт_ДанныеРегистра.КоличествоПакетов, 0)) КАК КоличествоПакетов,
	(ЕСТЬNULL(врт_ДанныеРегистра.Дельта1, 0)) КАК Дельта1,
	(ЕСТЬNULL(врт_ДанныеРегистра.Дельта2, 0)) КАК Дельта2,
	(ЕСТЬNULL(врт_ДанныеРегистра.Дельта3, 0)) КАК Дельта3,
	(ЕСТЬNULL(врт_ДанныеРегистра.Дельта4, 0)) КАК Дельта4}

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	врт_ДниОтчета.Период,
	ЕСТЬNULL(врт_ДанныеРегистра.ТипДанных, ""),
	ЕСТЬNULL(врт_ДанныеРегистра.КоличествоПакетов, 0),
	ЕСТЬNULL(врт_ДанныеРегистра.Дельта1, 0),
	ЕСТЬNULL(врт_ДанныеРегистра.Дельта2, 0),
	ЕСТЬNULL(врт_ДанныеРегистра.Дельта3, 0),
	ЕСТЬNULL(врт_ДанныеРегистра.Дельта4, 0)
ИЗ
	врт_ДниОтчета КАК врт_ДниОтчета
		ЛЕВОЕ СОЕДИНЕНИЕ врт_ДанныеРегистра2 КАК врт_ДанныеРегистра
		ПО врт_ДниОтчета.Период = врт_ДанныеРегистра.ПериодДень
ГДЕ
	&amp;РежимОтчета = 1
{ГДЕ
	врт_ДниОтчета.Период,
	(ЕСТЬNULL(врт_ДанныеРегистра.ТипДанных, "")) КАК ТипДанных,
	(ЕСТЬNULL(врт_ДанныеРегистра.КоличествоПакетов, 0)) КАК КоличествоПакетов,
	(ЕСТЬNULL(врт_ДанныеРегистра.Дельта1, 0)) КАК Дельта1,
	(ЕСТЬNULL(врт_ДанныеРегистра.Дельта2, 0)) КАК Дельта2,
	(ЕСТЬNULL(врт_ДанныеРегистра.Дельта3, 0)) КАК Дельта3,
	(ЕСТЬNULL(врт_ДанныеРегистра.Дельта4, 0)) КАК Дельта4}</query>
		<autoFillFields>false</autoFillFields>
	</dataSet>
