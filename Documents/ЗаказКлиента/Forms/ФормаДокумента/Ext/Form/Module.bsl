// << 07.09.2022 Марченко С.Н., КРОК, JIRA№A2105505-416
&НаСервере
Процедура КР_ОграничитСписокВыбораЭлементаСтатус()
	
	// Форма может быть заблокирована польностью, нет смысла что-то делать
	Если ТолькоПросмотр 
		Или Элементы.Статус.ТолькоПросмотр
		Или Не Элементы.Статус.Доступность 
		Тогда 
		Возврат;
	КонецЕсли;	
	
	Элементы.Статус.РежимВыбораИзСписка = Истина;
	СписокВыбора = Элементы.Статус.СписокВыбора;
	МенеджерДокумента = Документы.ЗаказКлиента;
	
	ДоступныеСтатусы = МенеджерДокумента.КР_СтатусыТекущейСистемы();   
	
	Если ЭтотОбъект.КР_УТ10_УТ11_ОграничитьДоступностьФормы Тогда   
		
		СписокВыбора.Очистить();   
		
		// Дополним статусами внешних систем так как документ мог было изменен ими
		ОбщегоНазначенияКлиентСервер.ДополнитьМассив(ДоступныеСтатусы, 
			МенеджерДокумента.КР_СтатусыВнешнихСистем());
		
		// Добавляем все статусы из последовательности до текущего (включительно)
		Для Итератор = 0 По ДоступныеСтатусы.Найти(Объект.Статус) Цикл 
			СписокВыбора.Добавить(ДоступныеСтатусы[Итератор]);
		КонецЦикла;
	Иначе
		СписокВыбора.ЗагрузитьЗначения(ДоступныеСтатусы);
	КонецЕсли;	
	
